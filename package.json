{
  "name": "surveyviewer",
  "displayName": "CASE Survey Viewer",
  "publisher": "coneno",
  "description": "",
  "version": "0.5.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:surveyviewer.preview"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/case-framework/vsc-survey-viewer-plugin.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "JsonFile",
        "aliases": [
          "JsonFile",
          "dc"
        ],
        "extensions": [
          ".json"
        ]
      }
    ],
    "commands": [
      {
        "command": "surveyviewer.preview",
        "category": "Survey Viewer",
        "title": "Survey Preview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm-run-all compile:* && tsc -p . && webpack --mode development",
    "watch": "npm-run-all -p watch:* && tsc -watch -p ./ && webpack --watch --mode development",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile:extension": "tsc -p ./",
    "compile:views": "webpack --mode development",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "webpack --watch --mode development",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@fontsource/open-sans": "^4.5.12",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.24.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "save-dev": "^0.0.1-security",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.7",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@monaco-editor/react": "^4.4.6",
    "bootstrap": "^5.2.1",
    "case-web-ui": "^1.13.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.2",
    "survey-engine": "^1.1.7"
  }
}
