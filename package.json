{
    "name": "jsonpreview",
    "displayName": "jsonPreview",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:jsonpreview.preview"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "JsonFile",
                "aliases": [
                    "JsonFile",
                    "dc"
                ],
                "extensions": [
                    ".json"
                ]
            }
        ],
        "commands": [
            {
                "command": "jsonpreview.preview",
                "title": "Json Preview"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npm-run-all compile:* && tsc -p . && webpack --mode development",
        "watch": "npm-run-all -p watch:* && tsc -watch -p ./ && webpack --watch --mode development",
        "compile:extension": "tsc -p ./",
        "compile:views": "webpack --mode development",
        "watch:extension": "tsc -watch -p ./",
        "watch:views": "webpack --watch --mode development",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "-": "^0.0.1",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.13",
        "@types/vscode": "^1.64.0",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "@vscode/test-electron": "^2.1.2",
        "css-loader": "^6.6.0",
        "eslint": "^8.9.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.1",
        "npm-run-all": "^4.1.5",
        "save-dev": "^0.0.1-security",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.7",
        "typescript": "^4.5.5",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "bootstrap": "^5.1.3",
        "case-web-ui": "^1.10.2",
        "date-fns": "^2.28.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-dropzone": "^12.0.4",
        "survey-engine": "^1.1.4"
    }
}
